      "body": {
        "bodyType": "Text",
        "autoSetLength": true,
        "textBody": "  {\n            \"description\": \"A Sample Activity containing a single shared writing bric\",\n            \"targetSelectorValues\": [ \"SWQ1-cntr\" ],\n\n            \"sequenceNodeKey\": \"b3d5b510-34dd-480f-b8e1-b474522c9b03\",\n            \"maxAttempts\": 3,\n\n            \"containerConfig\":\n            [\n                {\n                    \"description\": \"container for the shared writing question\",\n                    \"containerId\": \"SWQ1-cntr\",\n                    \"brixConfig\":\n                    [\n                        {\n                            \"bricId\": \"swq1\",\n                            \"bricType\": \"SharedWritingQuestion\",\n                            \"config\":\n                            {\n                                \"id\": \"swq-00001\",\n                                \"prompt\": \"How would you characterize the state of the magic forest today and why?\",\n                                \"minChars\": 8\n                            },\n                            \"configFixup\":\n                            [\n                                {\n                                    \"type\": \"set-property\",\n                                    \"name\": \"questionId\",\n                                    \"value\": { \"type\": \"ref\", \"domain\": \"info\", \"refId\": \"sequenceNodeKey\" }\n                                }\n                            ],\n                            \"answerKey\":\n                            {\n                                \"assessmentType\": \"discussions\",\n                                \"answers\":\n                                {\n                                    \"masterTopicId\": \"01f60e54-2fc2-4bce-a292-a2f545c9bf48\"\n                                }\n                            }\n                        }\n                    ],\n                    \"mortarConfig\":\n                    [\n                        {\n                            \"mortarId\": \"interaction\",\n                            \"mortarType\": \"InteractionLifecycleEvent\",\n                            \"config\":\n                            {\n                                \"id\": \"swInteraction\",\n                            },\n                            \"configFixup\":\n                            [\n                                {\n                                    \"type\": \"set-property\",\n                                    \"name\": \"topic\",\n                                    \"value\":\n                                    {\n                                        \"type\": \"brix-topic\",\n                                        \"bricType\": \"SharedWritingQuestion\",\n                                        \"instanceId\": \"swq-00001\",\n                                        \"eventName\": \"value-changed\"\n                                    }\n                                },\n                                {\n                                    \"type\": \"set-property\",\n                                    \"name\": \"seqNodeKey\",\n                                    \"value\": { \"type\": \"ref\", \"domain\": \"info\", \"refId\": \"sequenceNodeKey\" }\n                                },\n                                {\n                                    \"type\": \"set-property\",\n                                    \"name\": \"requestManager\",\n                                    \"value\": { \"type\": \"request-manager\" }\n                                }\n                            ]\n                        }\n                    ],\n                    \"hookupActions\":\n                    [\n                        {\n                            \"description\": \"Hookup the request manager to evaluate answers\",\n                            \"type\": \"method-call\",\n                            \"instance\": { \"type\": \"request-manager\" },\n                            \"methodName\": \"handleRequestsFrom\",\n                            \"args\":\n                            [\n                                { \"type\": \"ref\", \"domain\": \"brix\", \"refId\": \"swq1\" }\n                            ]\n                        },\n                        {\n                            \"description\": \"Hookup the request manager to handle requests for discussion posts\",\n                            \"type\": \"method-call\",\n                            \"instance\": { \"type\": \"request-manager\" },\n                            \"methodName\": \"handleGetPostsRequestsFrom\",\n                            \"args\":\n                            [\n                                { \"type\": \"ref\", \"domain\": \"brix\", \"refId\": \"swq1\" }\n                            ]\n                        },\n                        {\n                            \"description\": \"Hookup the request manager to handle requests for adding topic comments\",\n                            \"type\": \"method-call\",\n                            \"instance\": { \"type\": \"request-manager\" },\n                            \"methodName\": \"handleAddTopicCommentRequestsFrom\",\n                            \"args\":\n                            [\n                                { \"type\": \"ref\", \"domain\": \"brix\", \"refId\": \"swq1\" }\n                            ]\n                        },\n                        {\n                            \"description\": \"Draw the question\",\n                            \"type\": \"method-call\",\n                            \"instance\": {\"type\": \"ref\", \"domain\": \"brix\", \"refId\": \"swq1\"},\n                            \"methodName\": \"draw\",\n                            \"args\":\n                            [\n                                { \"type\": \"d3select\", \"targetSelectorIndex\": 0 }\n                            ]\n                        }\n                    ]\n                }\n            ]\n        }\n"
